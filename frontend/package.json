{
  "name": "frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:common": "vite dev --config packages/common/vite.config.ts",
    "dev:web": "vite dev --config vite.web.config.ts",
    "build": "npm run build:prod",
    "build:dev": "npm run clean:bundles && npm run build:request:dev && npm run build:common:dev && npm run build:components:dev && npm run build:web-bridge:dev && npm run build:web:dev && npm run copy:react-umd",
    "build:prod": "npm run clean:bundles && npm run build:request:prod && npm run build:common:prod && npm run build:components:prod && npm run build:web-bridge:prod && npm run build:web:prod && npm run copy:react-umd",
    "build:request": "npm run build:request:prod",
    "build:request:dev": "vite build --config packages/request/vite.config.ts --mode development",
    "build:request:prod": "vite build --config packages/request/vite.config.ts --mode production",
    "build:common": "npm run build:common:prod",
    "build:common:dev": "vite build --config packages/common/vite.config.ts --mode development",
    "build:common:prod": "vite build --config packages/common/vite.config.ts --mode production",
    "build:components": "npm run build:components:prod",
    "build:components:dev": "vite build --config packages/components/vite.config.ts --mode development",
    "build:components:prod": "vite build --config packages/components/vite.config.ts --mode production",
    "build:web-bridge": "npm run build:web-bridge:prod",
    "build:web-bridge:dev": "vite build --config packages/web-bridge/vite.config.ts --mode development",
    "build:web-bridge:prod": "vite build --config packages/web-bridge/vite.config.ts --mode production",
    "build:web": "npm run build:web:prod",
    "build:web:dev": "vite build --config vite.web.config.ts --mode development",
    "build:web:prod": "vite build --config vite.web.config.ts --mode production",
    "clean:bundles": "node scripts/clean-bundles.js",
    "copy:react-umd": "node scripts/copy-react-umd.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "axios-auth-refresh": "^3.3.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^2.1.8"
  }
}
